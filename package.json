{
  "name": "area49-nano-banana-ui",
  "version": "1.0.3",
  "description": "AREA49 - Nano Banana UI",
  "main": "electron-main.cjs",
  "author": "Artin Lesani",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "dist": "tsc && vite build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "electron-updater": "^6.3.9",
    "lucide-react": "^0.344.0",
    "three": "^0.165.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/three": "^0.165.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "electron": "^28.2.0",
    "electron-builder": "^24.12.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "build": {
    "appId": "com.area49.nanobanana",
    "productName": "AREA49 - Nano Banana UI",
    "electronDist": "./node_modules/electron/dist",
    "electronVersion": "28.3.3",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "electron-main.cjs",
      "electron-main.js",
      "preload.cjs",
      "preload.js",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron-main.cjs"
    },
    "copyright": "Copyright Â© 2025 Artin Lesani",
    "win": {
      "target": "nsis",
      "icon": "build/Area49_logo_A49-2024-3.ico",
      "signingHashAlgorithms": [],
      "sign": null,
      "executableName": "AREA49-Nano-Banana-UI",
      "artifactName": "AREA49-Nano-Banana-UI-${version}.${ext}",
      "publisherName": "Artin Lesani",
      "legalTrademarks": "AREA49 - Nano Banana UI",
      "verifyUpdateCodeSignature": false
    },
    "mac": {
      "target": [
        "dmg",
        "pkg"
      ],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png",
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "artinlesani73-dev",
        "repo": "APP-for-NANO",
        "releaseType": "release"
      }
    ]
  }
}
