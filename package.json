{
  "name": "image-provenance-studio",
  "version": "1.0.0",
  "description": "Professional Image Generation Tool",
  "main": "electron-main.cjs",
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
